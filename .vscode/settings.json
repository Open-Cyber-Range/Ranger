{
  "rust-analyzer.checkOnSave.command": "clippy",
  "editor.formatOnSave": true,
  "xo.enable": true,
  "xo.format.enable": true,
  "[javascript]": {
    "editor.defaultFormatter": "samverschueren.linter-xo"
  },
  "[typescript]": {
    "editor.defaultFormatter": "samverschueren.linter-xo"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "samverschueren.linter-xo"
  },
  "sqltools.connections": [
    {
      "mysqlOptions": {
        "authProtocol": "default"
      },
      "previewLimit": 50,
      "server": "mariadb",
      "port": 3306,
      "driver": "MariaDB",
      "name": "mariadb",
      "database": "ranger",
      "username": "mysql_user",
      "password": "mysql_pass"
    }
  ]
}